{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mind-schema.org/schema/primitives/vector3.json",
  "title": "vector3",
  "description": "Three-dimensional vector with X, Y, Z coordinates.",
  "type": "object",
  "properties": {
    "x": { "$ref": "https://mind-schema.org/schema/primitives/coordinate.json" },
    "y": { "$ref": "https://mind-schema.org/schema/primitives/coordinate.json" },
    "z": { "$ref": "https://mind-schema.org/schema/primitives/coordinate.json" }
  },
  "required": ["x", "y", "z"],
  "additionalProperties": false,
  "x-ai": {
    "dims": 3,
    "dtype": "float32",
    "unit": "metre",
    "order": ["x", "y", "z"],
    "embeddingHint": "euclidean-position"
  }
}
