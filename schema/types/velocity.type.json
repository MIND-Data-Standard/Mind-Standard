{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mind-schema.org/schema/types/velocity.json",
  "title": "velocity",
  "description": "Linear and angular velocity vectors for tracked objects.",
  "type": "object",
  "properties": {
    "linear": { "$ref": "https://mind-schema.org/schema/primitives/vector3.json", "description": "Linear velocity vector (metres/second)." },
    "angular": { "$ref": "https://mind-schema.org/schema/primitives/vector3.json", "description": "Angular velocity vector (radians/second)." }
  },
  "required": ["linear", "angular"],
  "additionalProperties": false,
  "x-ai": {
    "dims": 6,
    "dtype": "float32",
    "order": ["linear.x", "linear.y", "linear.z", "angular.x", "angular.y", "angular.z"],
    "units": { "linear": "metre/second", "angular": "radian/second" },
    "embeddingHint": "linear+angular-velocity"
  }
}
